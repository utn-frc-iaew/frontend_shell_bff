# Server Configuration
PORT=4001
NODE_ENV=development

# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_AUDIENCE=https://your-api-identifier

# Auth0 Machine-to-Machine (B2B)
AUTH0_CLIENT_ID=your-m2m-client-id
AUTH0_CLIENT_SECRET=your-m2m-client-secret
AUTH0_TOKEN_URL=https://your-tenant.auth0.com/oauth/token

# Internal APIs
API_USER_URL=http://localhost:5001
API_CUSTOMER_URL=http://localhost:5002
API_USER_AUDIENCE=https://api-user
API_CUSTOMER_AUDIENCE=https://api-customer
