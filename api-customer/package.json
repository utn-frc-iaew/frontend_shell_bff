{
  "name": "api-customer",
  "version": "1.0.0",
  "description": "Customer API with NestJS and MongoDB",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
